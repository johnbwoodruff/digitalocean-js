{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*",
    "node_modules/**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "excludedFiles": [
        "node_modules/**/*"
      ],
      "rules": {
        "@typescript-eslint/no-non-null-assertion": "off"
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "excludedFiles": [
        "node_modules/**/*"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "excludedFiles": [
        "node_modules/**/*"
      ],
      "rules": {}
    }
  ]
}
